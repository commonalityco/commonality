  "version": "0.0.3",
  "publishConfig": {
    "access": "public"
  },
  "types": "dist/commonalityco-data-project.cjs.d.ts",
  "main": "dist/commonalityco-data-project.cjs.js",
  "module": "dist/commonalityco-data-project.esm.js",
  "exports": {
