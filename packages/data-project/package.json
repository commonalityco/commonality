    "@commonalityco/types": "^0.0.10",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@commonalityco/utils-core": "^0.0.3",
    "chalk": "^5.3.0",
    "find-up": "^6.3.0",
    "fs-extra": "^11.1.1",
    "globby": "^13.2.2",
    "yaml": "^2.3.1",
    "zod": "^3.22.0"
  }
