{
  "name": "@commonalityco/utils-core",
  "version": "0.0.11",
  "type": "module",
  "description": "Contains the common constants used in all Commonality applications and packages",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": "./dist/index.js",
      "./ui": "./dist/ui/index.ts"
    }
  },
  "types": "./src/index.ts",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./ui": "./src/ui/index.ts"
