  },
  "devDependencies": {
    "@commonalityco/config-tsconfig": "workspace:*",
    "eslint-config-commonality": "workspace:*",
    "typescript": "^5.2.2",
    "@types/react": "^18.2.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1"
