{
  "name": "@commonalityco/data-packages",
  "description": "Get package information about a local project",
  "version": "0.2.46",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": "./dist/index.js"
    }
  },
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "main": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "fs-extra": "^11.2.0"
  },
  "peerDependencies": {
    "@commonalityco/data-project": "workspace:*",
    "@commonalityco/utils-core": "workspace:*"
  },
  "devDependencies": {
    "@commonalityco/config-tsconfig": "workspace:*",
    "@commonalityco/data-project": "workspace:*",
    "@commonalityco/types": "workspace:*",
    "@commonalityco/utils-core": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.20",
    "eslint-config-commonality": "workspace:*",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commonalityco/commonality",
    "directory": "packages/data-packages"
  }
}