{
  "name": "@commonalityco/studio",
  "version": "0.2.4",
  "types": "dist/commonalityco-studio.cjs.d.ts",
  "main": "dist/commonalityco-studio.cjs.js",
  "module": "dist/commonalityco-studio.esm.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/commonalityco-studio.cjs.mjs",
        "default": "./dist/commonalityco-studio.cjs.js"
      },
      "module": "./dist/commonalityco-studio.esm.js",
      "import": "./dist/commonalityco-studio.cjs.mjs",
      "default": "./dist/commonalityco-studio.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "NODE_ENV=development node server.js",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "NODE_ENV=production node dist/dev-start.js",
    "lint": "next lint",
    "e2e:run-script": "node dist/dev-start.js",
    "e2e": "pnpm run build:script &&  start-server-and-test e2e:run-script http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "pnpm run build:script && start-server-and-test e2e:run-script http://localhost:3000 \"cypress run --e2e\""
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts"
    ]
  },
  "dependencies": {
    "@commonalityco/data-codeowners": "*",
    "@commonalityco/data-documents": "*",
    "@commonalityco/data-graph": "*",
    "@commonalityco/data-graph-worker": "*",
    "@commonalityco/data-packages": "*",
    "@commonalityco/data-project": "*",
    "@commonalityco/data-tags": "*",
    "@commonalityco/data-violations": "*",
    "@commonalityco/feature-graph": "*",
    "@commonalityco/ui-core": "*",
    "@commonalityco/ui-design-system": "*",
    "@commonalityco/ui-graph": "*",
    "@commonalityco/utils-core": "*",
    "@commonalityco/utils-graph": "*",
    "@commonalityco/utils-package": "*",
    "execa": "7.2.0",
    "fs-extra": "^11.1.1",
    "get-port": "5.1.1",
    "lodash": "^4.17.21",
    "next": "13.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    ".next/",
    "next.config.js",
    "server.js",
    "dist/"
  ],
  "devDependencies": {
    "@commonalityco/config-eslint": "^0.0.7",
    "@commonalityco/config-tailwind": "^0.0.4",
    "@commonalityco/types": "^0.0.10",
    "@tanstack/react-query": "^4.32.6",
    "@tanstack/react-query-devtools": "^4.32.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.197",
    "@types/lodash.sortby": "^4.7.7",
    "@types/node": "^18.17.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/resolve": "^1.20.2",
    "@xstate/react": "^3.2.2",
    "autoprefixer": "^10.4.15",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "cypress": "^12.17.3",
    "cytoscape": "^3.26.0",
    "eslint": "^8.47.0",
    "eslint-config-next": "13.1.6",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "lucide-react": "^0.248.0",
    "next-themes": "^0.2.1",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "postcss": "^8.4.27",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "server-only": "^0.0.1",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.5",
    "xstate": "^4.38.2"
  }
}
