{
  "name": "@commonalityco/studio",
  "version": "0.2.6",
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/index.d.mts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "scripts": {
    "dev": "NODE_ENV=development node server.js",
    "build": "next build && tsc scripts/index.mts --outDir dist --target ES2020 --module esnext --moduleResolution Node16 --declaration --esModuleInterop",
    "analyze": "ANALYZE=true next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "e2e:run-script": "node dist/dev-start.js",
    "e2e": "pnpm run build:script &&  start-server-and-test e2e:run-script http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "pnpm run build:script && start-server-and-test e2e:run-script http://localhost:3000 \"cypress run --e2e\""
  },
  "dependencies": {
    "@commonalityco/data-codeowners": "^0.0.10",
    "@commonalityco/data-documents": "^0.0.6",
    "@commonalityco/data-graph": "^0.0.6",
    "@commonalityco/data-graph-worker": "^0.0.4",
    "@commonalityco/data-packages": "^0.0.11",
    "@commonalityco/data-project": "^0.0.5",
    "@commonalityco/data-tags": "^0.0.6",
    "@commonalityco/data-violations": "^0.0.10",
    "@commonalityco/feature-graph": "^0.0.7",
    "@commonalityco/ui-core": "^0.0.8",
    "@commonalityco/ui-design-system": "^0.0.5",
    "@commonalityco/ui-graph": "^0.0.8",
    "@commonalityco/utils-core": "^0.0.5",
    "@commonalityco/utils-graph": "^0.0.5",
    "@commonalityco/utils-package": "^0.0.5",
    "execa": "^7.2.0",
    "fs-extra": "^11.1.1",
    "get-port": "^7.0.0",
    "lodash": "^4.17.21",
    "next": "13.4.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    ".next/",
    "next.config.js",
    "server.js",
    "dist/"
  ],
  "devDependencies": {
    "@commonalityco/config-tailwind": "^0.0.5",
    "@commonalityco/types": "^0.0.12",
    "@tanstack/react-query": "^4.32.6",
    "@tanstack/react-query-devtools": "^4.32.6",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.197",
    "@types/lodash.sortby": "^4.7.7",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/resolve": "^1.20.2",
    "@xstate/react": "^3.2.2",
    "autoprefixer": "^10.4.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cypress": "^12.17.4",
    "cytoscape": "^3.26.0",
    "eslint": "^8.47.0",
    "eslint-config-commonality": "^0.0.8",
    "eslint-config-next": "13.4.16",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "lucide-react": "^0.268.0",
    "next-themes": "^0.2.1",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "server-only": "^0.0.1",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "xstate": "^4.38.2"
  }
