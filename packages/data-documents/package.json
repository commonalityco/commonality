      },
      "module": "./dist/commonalityco-data-documents.esm.js",
      "import": "./dist/commonalityco-data-documents.cjs.mjs",
      "default": "./dist/commonalityco-data-documents.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
