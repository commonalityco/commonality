    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@commonalityco/config-eslint": "*",
    "@commonalityco/config-tsconfig": "*",
    "@commonalityco/types": "*",
    "@commonalityco/utils-core": "*",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.17.5",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@commonalityco/data-project": "*",
    "fs-extra": "^11.1.1",
    "globby": "11.0.4"
  }
