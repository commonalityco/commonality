{
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**", "storybook-static/**"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "lint:fix": {
      "dependsOn": ["^build"]
    },
    "type-check": {
      "dependsOn": ["^build"]
    },
    "e2e": {
      "dependsOn": ["build"]
    }
  }
}
